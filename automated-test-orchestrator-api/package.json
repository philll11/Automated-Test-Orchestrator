{
  "name": "api-service",
  "version": "2.0.0",
  "description": "The backend orchestration service.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.build.json",
    "dev": "node --loader ts-node/esm src/index.ts",
    "test": "jest --verbose",
    "test:unit": "jest --testMatch=\"<rootDir>/src/**/*.unit.test.ts\" --verbose",
    "test:int": "jest --runInBand --testMatch=\"<rootDir>/src/**/*.integration.test.ts\" --verbose",
    "test:e2e:creds": "jest --runInBand --testMatch=\"<rootDir>/src/**/credentials.e2e.test.ts\" --verbose",
    "test:e2e:disc": "jest --runInBand --testMatch=\"<rootDir>/src/**/discovery.e2e.test.ts\" --verbose",
    "test:e2e:exec": "jest --runInBand --testMatch=\"<rootDir>/src/**/execution.e2e.test.ts\" --verbose",
    "test:e2e:map": "jest --runInBand --testMatch=\"<rootDir>/src/**/mappings.e2e.test.ts\" --verbose",
    "test:e2e:result": "jest --runInBand --testMatch=\"<rootDir>/src/**/test_execution_results.e2e.test.ts\" --verbose",
    "test:e2e:plan": "jest --runInBand --testMatch=\"<rootDir>/src/**/test_plans.e2e.test.ts\" --verbose",
    "test:e2e:live": "jest --runInBand --testMatch=\"<rootDir>/src/**/live_system.e2e.test.ts\" --verbose",
    "test:e2e:all": "jest --runInBand --testMatch=\"<rootDir>/src/**/*.e2e.test.ts\" --verbose"
  },
  "dependencies": {
    "@types/express": "4.17.21",
    "@types/pg": "8.11.6",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "axios": "1.7.2",
    "express": "4.19.2",
    "inversify": "7.9.1",
    "p-limit": "7.1.1",
    "pg": "8.12.0",
    "reflect-metadata": "0.2.2",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@types/jest": "29.5.14",
    "@types/node": "20.14.9",
    "@types/supertest": "6.0.3",
    "@types/uuid": "10.0.0",
    "babel-jest": "29.7.0",
    "dotenv": "16.4.5",
    "jest": "29.7.0",
    "nock": "14.0.10",
    "supertest": "7.1.4",
    "ts-jest": "29.1.5",
    "ts-node": "10.9.2",
    "typescript": "5.5.2"
  }
}
