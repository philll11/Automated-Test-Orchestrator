{
  "name": "orchestrator-application",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "ato": "./dist/cli/cli.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "node --loader ts-node/esm src/index.ts",
    "test": "jest --verbose",
    "test:unit:test_plan": "jest --testMatch=\"**/test_plan_service.unit.test.ts\" --verbose",
    "test:unit:mapping": "jest --testMatch=\"**/mapping_service.unit.test.ts\" --verbose",
    "test:unit:credential": "jest --testMatch=\"**/credential_service.unit.test.ts\" --verbose",
    "test:unit:exe_result": "jest --testMatch=\"**/test_execution_result_service.unit.test.ts\" --verbose",
    "test:unit:all": "npm run test:unit:mapping && npm run test:unit:test_plan && npm run test:unit:credential && npm run test:unit:exe_result",
    "test:int:test_plan": "jest --testMatch=\"**/test_plan_repository.integration.test.ts\" --verbose",
    "test:int:component": "jest --testMatch=\"**/mapping_repository.integration.test.ts\" --verbose",
    "test:int:discovery": "jest --testMatch=\"**/discovered_component_repository.integration.test.ts\" --verbose",
    "test:int:exe_result": "jest --testMatch=\"**/test_execution_result_repository.integration.test.ts\" --verbose",
    "test:int:all": "npm run test:int:test_plan && npm run test:int:component && npm run test:int:discovery && npm run test:int:exe_result",
    "test:e2e:discover": "jest --testMatch=\"**/discovery.e2e.test.ts\" --verbose",
    "test:e2e:execute": "jest --testMatch=\"**/execution.e2e.test.ts\" --verbose",
    "test:e2e:live": "jest --testMatch=\"**/live_system.e2e.test.ts\" --verbose",
    "test:e2e:mapping": "jest --testMatch=\"**/mappings.e2e.test.ts\" --verbose",
    "test:e2e:credential": "jest --testMatch=\"**/credentials.e2e.test.ts\" --verbose",
    "test:e2e:exe_result": "jest --testMatch=\"**/test_execution_results.e2e.test.ts\" --verbose",
    "test:e2e:test_plan": "jest --testMatch=\"**/test_plans.e2e.test.ts\" --verbose",
    "test:e2e:all": "npm run test:e2e:discover && npm run test:e2e:execute && npm run test:e2e:live && npm run test:e2e:mapping && npm run test:e2e:credential && npm run test:e2e:exe_result && npm run test:e2e:test_plan"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@types/inquirer": "9.0.9",
    "@types/jest": "29.5.12",
    "@types/keytar": "4.4.0",
    "@types/node": "20.14.9",
    "@types/supertest": "6.0.3",
    "@types/uuid": "10.0.0",
    "babel-jest": "29.7.0",
    "dotenv": "16.4.5",
    "jest": "29.7.0",
    "nock": "14.0.10",
    "supertest": "7.1.4",
    "ts-jest": "29.1.5",
    "ts-node": "10.9.2",
    "typescript": "5.5.2",
    "uuid": "13.0.0"
  },
  "dependencies": {
    "@types/express": "4.17.21",
    "@types/pg": "8.11.6",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "axios": "1.7.2",
    "chalk": "5.6.2",
    "commander": "12.1.0",
    "express": "4.19.2",
    "inquirer": "12.9.4",
    "inversify": "7.9.1",
    "ora": "8.2.0",
    "pg": "8.12.0",
    "reflect-metadata": "0.2.2",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1"
  }
}
